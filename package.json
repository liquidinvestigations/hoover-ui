{
  "name": "hoover-ui",
  "version": "0.8.0",
  "description": "React.js port of the hoover ui",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 8000 -H 0.0.0.0",
    "start": "next start -p 8000 -H 0.0.0.0",
    "prod": "cross-env NODE_ENV=production next build && next start -p 8000 -H 0.0.0.0",
    "test": "cross-env NODE_ENV=test jest",
    "test:update": "cross-env NODE_ENV=test jest -u",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "tdd": "yarn test:watch",
    "heroku-postbuild": "next build"
  },
  "engine": {
    "node": "14.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoover/ui"
  },
  "dependencies": {
    "@date-io/luxon": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@svgr/webpack": "^5.5.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "classnames": "^2.2.6",
    "copy-text-to-clipboard": "^2.2.0",
    "express": "^4.17.1",
    "fast-deep-equal": "^3.1.3",
    "finderjs": "^1.3.0",
    "http-proxy-middleware": "^1.0.6",
    "langs": "^2.0.0",
    "lodash": "^4.17.20",
    "lodash-webpack-plugin": "^0.11.6",
    "lucene": "^2.1.1",
    "luxon": "^1.25.0",
    "material-ui-chip-input": "^1.1.0",
    "next": "10.0.5",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-hotkeys": "^2.0.0",
    "react-modal": "^3.12.1",
    "react-placeholder": "^4.0.3",
    "react-split-pane": "^0.1.92"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "react-test-renderer": "^17.0.1",
    "url-loader": "^4.1.1"
  }
}
