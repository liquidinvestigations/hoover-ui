{
  "name": "hoover-ui",
  "version": "0.22.1",
  "description": "React.js port of the hoover ui",
  "main": "index.js",
  "scripts": {
    "build": "next build && next export && node generate-nginx-config.js",
    "dev": "next dev -p 8000 -H 0.0.0.0",
    "start": "next start -p 8000 -H 0.0.0.0",
    "prod": "cross-env NODE_ENV=production next build && next start -p 8000 -H 0.0.0.0",
    "test": "cross-env NODE_ENV=test jest",
    "test:update": "cross-env NODE_ENV=test jest -u",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "postinstall": "node postinstall-fixes.js",
    "lint-next": "next lint",
    "lint": "next lint && eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "engine": {
    "node": "18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoover/ui"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
